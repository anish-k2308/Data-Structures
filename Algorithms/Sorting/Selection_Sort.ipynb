   "source": [
    "def selection_sort(arr):\n",
    "    for i in range(len(arr)-1):\n",
    "        mine = i\n",
    "        for j in range(i+1,len(arr)):\n",
    "            if arr[j]<arr[mine]:\n",
    "                mine  = j\n",
    "        arr[i],arr[mine] = arr[mine],arr[i]\n",
    "    return arr"
   ]
