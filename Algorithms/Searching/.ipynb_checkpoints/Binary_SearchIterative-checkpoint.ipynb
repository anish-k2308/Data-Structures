    "def binary_searchi(arr,x):\n",
    "    h = len(arr) \n",
    "    l = 0\n",
    "    while (l<h):\n",
    "        m =  (l+h)//2\n",
    "        if (x == arr[m]):\n",
    "            return m\n",
    "        elif(x < arr[m]):\n",
    "            h = m-1\n",
    "        else:\n",
    "            l = m+1\n",
    "    return -1"
   ]
